<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nitklaundry.userapp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="com.example.nitklaundry.userapp.userapp"
        android:allowBackup="true"
        android:icon="@drawable/laundry"
        android:label="Smart Laundry"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.nitklaundry.userapp.LoginActivity"
            android:label="@string/app_name"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nitklaundry.userapp.UserPage"
            android:label="Smart Laundry"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nitklaundry.userapp.RateList"
            android:label="@string/title_activity_rate_list"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.OngoingOrders"
            android:label="@string/title_activity_ongoing_orders"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.MonthlyBills"
            android:label="@string/title_activity_monthly_bills"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.History"
            android:label="@string/title_activity_history"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.Account"
            android:label="@string/title_activity_account"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.Settings"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.Information"
            android:label="@string/title_activity_information"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity
            android:name="com.example.nitklaundry.userapp.Place_Order"
            android:label="@string/title_activity_place__order"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name="com.example.nitklaundry.userapp.Register" />
        <activity
            android:name="com.example.nitklaundry.userapp.Account_Edit"
            android:label="@string/title_activity_account__edit"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
        <activity android:name="com.example.nitklaundry.userapp.place_order2" />
        <activity
            android:name="com.example.nitklaundry.userapp.OrderNotifications"
            android:label="@string/title_activity_order_notifications"
            android:theme="@style/AppTheme.NoActionBar"
            android:configChanges="keyboardHidden|orientation|screenSize">

        </activity>
    </application>

</manifest>